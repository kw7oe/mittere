   <children>
      <Label fx:id="messageLabel" maxWidth="373.0" minWidth="30.0" style="-fx-background-color: #fafafc; -fx-background-radius: 6;" text="message" wrapText="true">
         <font>
            <Font name="Helvetica Neue" size="11.0" />
         </font>
         <padding>
            <Insets bottom="4.0" left="10.0" right="10.0" top="4.0" />
         </padding>
      </Label>
   </children>
</HBox>
